      "scope": ["constant.character.escape", "constant.other.placeholder"],
      "settings": {
        "foreground": "#CECDC3"
      }
    },
    {
      "name": "keywords",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#879A39"
      }
    },
    {
      "name": "keywordsControl",
      "scope": [
        "keyword.control.import",
        "keyword.control.from",
        "keyword.import"
      ],
      "settings": {
        "foreground": "#D14D41"
      }
    },
    {
      "name": "storageModifiers",
