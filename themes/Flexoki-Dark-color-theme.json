      "settings": {
        "foreground": "#D0A215"
      }
    },
    {
      "name": "types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#D0A215"
      }
    },
    {
      "name": "constants",
      "scope": ["variable.other.constant", "variable.readonly"],
